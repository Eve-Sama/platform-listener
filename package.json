{
  "name": "we-media-helper",
  "productName": "We Media Helper",
  "version": "0.0.8",
  "main": "electron/main.js",
  "description": "Grab account data via your cookie and support more function makes you focus on we media",
  "author": {
    "name": "Eve-Sama",
    "url": "https://github.com/Eve-Sama"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Eve-Sama/we-media-helper"
  },
  "scripts": {
    "start": "cross-env BROWSER=none react-scripts start & electron .",
    "build": "cross-env BUILD_PATH=./dist/web react-scripts build && electron-builder build --mac",
    "build2": "gulp build"
  },
  "build": {
    "appId": "com.electron.we-media-helper",
    "copyright": "Copyright Â© 2022 Eve-Sama",
    "generateUpdatesFilesForAllChannels": true,
    "directories": {
      "output": "./dist/installer"
    },
    "mac": {
      "target": {
        "target": "default",
        "arch": [
          "arm64",
          "x64",
          "universal"
        ]
      },
      "icon": "./assets/icons/mac-dock.icns",
      "publish": {
        "owner": "Eve-Sama",
        "repo": "we-media-helper",
        "provider": "github"
      }
    },
    "nsis": {
      "differentialPackage": false
    },
    "dmg": {
      "writeUpdateInfo": false
    },
    "files": [
      "**/*",
      "!dist/installer",
      "!**/*.ts",
      "!**/*.tsx",
      "!src/",
      "!public/",
      "!.eslintignore",
      "!.eslintrc.js",
      "!.gitignore",
      "!.prettierrc",
      "!gulpfile.ts",
      "!LICENSE",
      "!package-lock.json",
      "!README.md",
      "!tsconfig.json",
      "!yarn.lock"
    ],
    "extends": null
  },
  "dependencies": {
    "ahooks": "^3.7.1",
    "antd": "^4.23.1",
    "axios": "^1.1.3",
    "axios-esm": "^1.0.0",
    "compare-versions": "^5.0.1",
    "dayjs": "^1.11.5",
    "electron-store": "^8.1.0",
    "hotkeys-js": "^3.10.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.3.0",
    "react-scripts": "5.0.1",
    "uuid": "^9.0.0",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@types/gulp": "^4.0.9",
    "@types/lodash": "^4.14.185",
    "@types/node": "^16.11.58",
    "@types/react": "^18.0.18",
    "@types/react-dom": "^18.0.6",
    "@types/through2": "^2.0.36",
    "@types/uuid": "^8.3.4",
    "ansi-colors": "^4.1.3",
    "cross-env": "^7.0.3",
    "electron": "^21.1.0",
    "electron-builder": "^23.6.0",
    "electron-installer-dmg": "^4.0.0",
    "electron-installer-windows": "^3.0.0",
    "electron-packager": "^16.0.0",
    "electron-updater": "^5.3.0",
    "electron-winstaller": "^5.1.0",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "gulp": "^4.0.2",
    "prettier": "^2.7.1",
    "sass": "^1.54.9",
    "through2": "^4.0.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.3"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ],
    "plugins": [
      "react-hooks"
    ],
    "rules": {
      "react-hooks/rule-of-hooks": "error"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "homepage": "."
}
