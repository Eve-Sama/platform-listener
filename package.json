{
  "name": "platform-listener",
  "version": "0.0.1",
  "private": true,
  "main": "electron/main.js",
  "description": "Listen some imformation",
  "author": {
    "name": "Eve-Sama",
    "url": "https://github.com/Eve-Sama"
  },
  "scripts": {
    "start": "cross-env BROWSER=none react-scripts start & electron .",
    "build": "npm run build:packager && npm run build:dmg",
    "build:web": "cross-env BUILD_PATH=dist/web react-scripts build",
    "build:packager": "electron-packager ./dist/web 'Platform Listener' --platform=darwin --arch=arm64 --icon=build/icon/app.icns --overwrite=true --out=dist/electron",
    "build:dmg": "electron-installer-dmg ./dist/electron/'Platform Listener'-darwin-arm64/'Platform Listener'.app platform-listener-arm64 --overwrite --out=dist/electron"
  },
  "dependencies": {
    "ahooks": "^3.7.1",
    "antd": "^4.23.1",
    "axios-esm": "^1.0.0",
    "cross-env": "^7.0.3",
    "dayjs": "^1.11.5",
    "electron-store": "^8.1.0",
    "http-proxy-middleware": "^2.0.6",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.3.0",
    "react-scripts": "5.0.1",
    "uuid": "^9.0.0",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.185",
    "@types/node": "^16.11.58",
    "@types/react": "^18.0.18",
    "@types/react-dom": "^18.0.6",
    "@types/uuid": "^8.3.4",
    "electron": "^21.1.0",
    "electron-builder": "^23.6.0",
    "electron-installer-dmg": "^4.0.0",
    "electron-packager": "^16.0.0",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.7.1",
    "sass": "^1.54.9",
    "typescript": "^4.8.3"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "homepage": "."
}
